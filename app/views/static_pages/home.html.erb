<h1>StaticPages#home</h1>

<% if user_signed_in? %>
  <p> Connecté en tant que <%= current_user.email %> <p>
  <%= link_to("Se déconnecter", destroy_user_session_path, method: :delete) %> 
<% else %>
  <p> Déjà inscrit ? <%= link_to "S'identifier ?", new_user_session_path %> </p>
  <p> Nouveau ? <%= link_to "Créer un compte", new_user_registration_path %> </p>
<% end %>